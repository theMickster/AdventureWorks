{
  "EntityFrameworkCoreSettings": {
    "CommandLogLevel": "Warning",
    "CommandTimeout": "60",
    "CurrentConnectionStringName": "DefaultConnection"
  },
  "TokenSettings": {
    "Subject": "AdventureWorks API Access Token",
    "TokenExpirationInSeconds": 300,
    "Issuer": "http://localhost/",
    "Audience": "http://localhost/",
    "RefreshTokenExpirationInDays": 2
  },
  "KeyVault:VaultUri": "https://a-valid-akv-goes-here.vault.azure.net/",
  "ApplicationInsights:ConnectionString": "Azure Application Insights Connection String Goes Here",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Error",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Error",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "AllowedHosts": "*"
}